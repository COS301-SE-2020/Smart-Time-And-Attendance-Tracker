{
  "name": "stat-desktop",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "electron .",
    "start:electron": "ng build --base-href ./ && electron .",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "electron-packager --out winx64 --overwrite --platform win32 --appname clientlmcenter . --icon=dist/STAT-desktop/favicon.ico",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.1",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.0.1",
    "@angular/compiler": "~9.0.1",
    "@angular/core": "~9.0.1",
    "@angular/fire": "^6.0.2",
    "@angular/forms": "~9.0.1",
    "@angular/localize": "^9.1.11",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.0.1",
    "@angular/platform-browser-dynamic": "~9.0.1",
    "@angular/router": "~9.0.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "@types/gapi.auth2": "0.0.52",
    "@types/source-map": "^0.5.2",
    "active-win": "^6.2.0",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "child_process": "^1.0.2",
    "electron-active-window": "0.0.4",
    "firebase": "^7.20.0",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "gapi": "0.0.3",
    "gapi-client": "0.0.3",
    "iconv": "^3.0.0",
    "json2csv": "^5.0.1",
    "jspdf": "^2.0.0",
    "jspdf-autotable": "^3.5.9",
    "ngx-mat-select-search": "^3.0.2",
    "ngx-spinner": "^10.0.1",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "run": "^1.4.0",
    "rxjs": "~6.5.4",
    "stream": "0.0.2",
    "tslib": "^1.10.0",
    "xlsx": "^0.16.6",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.2",
    "@angular/cli": "~9.0.2",
    "@angular/compiler-cli": "~9.0.1",
    "@angular/language-service": "~9.0.1",
    "@types/gapi": "0.0.39",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron": "^10.1.3",
    "electron-rebuild": "^2.0.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  },
  "win": {
    "icon": "src/favicon.ico"
  }
}
